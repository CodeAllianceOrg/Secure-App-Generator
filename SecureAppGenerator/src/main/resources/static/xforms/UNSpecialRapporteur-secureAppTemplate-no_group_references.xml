<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms"
    xmlns:h="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:jr="http://openrosa.org/javarosa"
    >
  <h:head>
    <h:title>UN Special Rapporteur on extrajudicial, summary, or arbitrary execution</h:title>
    <model>
      <instance>
        <un_special_rapporteur id="un_special_rapporteur">
          <language/>
          <keywords/>
            
			<ConfidentialityNote/>
            <PersonInfo_Message/>
            <PersonInfo_Section jr:template="">
              <PersonInfo_FirstName/>
              <PersonInfo_LastName/>
              <PersonInfo_OtherNames/>
              <PersonInfo_Sex/>
              <PersonInfo_BirthDate/>
              <PersonInfo_Age/>
              <PersonInfo_Nationality/>
              <PersonInfo_CivilStatus/>
              <PersonInfo_DocumentType/>
              <PersonInfo_IDNumber/>
              <PersonInfo_IDIssuedBy/>
              <PersonInfo_IDIssueDate/>
              <PersonInfo_Linkage/>
              <PersonInfo_Address/>
              <PersonInfo_MiscComments/>
            </PersonInfo_Section>
            <ProfessionHistoryNote/>
            <ProfessionalHistoryGrid jr:template="">
              <ProfessionalInfo_FirstName/>
              <ProfessionalInfo_LastName/>
              <ProfessionalInfo_Profession/>
              <ProfessionalInfo_StartDate/>
              <ProfessionalInfo_EndDate/>
            </ProfessionalHistoryGrid>
            <PersonComments/>


            <IncidentDateStart/>
            <IncidentDateEnd/>
            <IncidentTime/>
            <IncidentLocation/>
            <Incidentmsg/>
            <IncidentDescA/>
            <IncidentDescB/>
            <IncidentDescC/>


            <ForcesResponsibleMsg/>
            <RespForcesA/>
            <RespForcesB/>
            <RespForcesC/>

			
            <ComplaintsMsg/>
            <ComplaintsGrid jr:template="">
              <Complaints_FiledDate/>
              <Complaints_FiledBy/>
              <Complaints_FiledOrganization/>
            </ComplaintsGrid>
            <OtherStepsTaken/>


            <StepsMsg/>
            <AuthoritiesStepsA/>
            <AuthoritiesStepsB/>


            <SubmitterFamilyName/>
            <SubmitterFirstName/>
            <OtherNameMsg2/>
            <SubmitterOtherName/>
            <SubmitterStatus/>
            <SpecifyOtherStatus/>
            <SubmitterAddress/>
            <SubmitterPhone/>
            <SubmitterFax/>
            <SubmitterEmail/>
            <ConfidentialNote/>
            <IDConfidential/>
            <QuestionnaireDate/>
            <SignatureReceived/>

          <today/>
          <meta>
            <instanceID/>
          </meta>
        </un_special_rapporteur>
      </instance>
	  
      <bind nodeset="/un_special_rapporteur/language" required="false()" type="select1"/>
      <bind nodeset="/un_special_rapporteur/keywords" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ConfidentialityNote" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Message" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_FirstName" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_LastName" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_OtherNames" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_Sex" required="false()" type="select1"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_BirthDate" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_Age" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_Nationality" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_CivilStatus" required="false()" type="select1"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_DocumentType" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_IDNumber" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_IDIssuedBy" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_IDIssueDate" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_Linkage" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_Address" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/PersonInfo_Section/PersonInfo_MiscComments" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ProfessionHistoryNote" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ProfessionalHistoryGrid/ProfessionalInfo_FirstName" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ProfessionalHistoryGrid/ProfessionalInfo_LastName" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ProfessionalHistoryGrid/ProfessionalInfo_Profession" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ProfessionalHistoryGrid/ProfessionalInfo_StartDate" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/ProfessionalHistoryGrid/ProfessionalInfo_EndDate" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/PersonComments" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/IncidentDateStart" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/IncidentDateEnd" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/IncidentTime" required="false()" type="time"/>
      <bind nodeset="/un_special_rapporteur/IncidentLocation" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/Incidentmsg" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/IncidentDescA" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/IncidentDescB" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/IncidentDescC" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ForcesResponsibleMsg" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/RespForcesA" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/RespForcesB" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/RespForcesC" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ComplaintsMsg" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ComplaintsGrid/Complaints_FiledDate" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/ComplaintsGrid/Complaints_FiledBy" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ComplaintsGrid/Complaints_FiledOrganization" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/OtherStepsTaken" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/StepsMsg" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/AuthoritiesStepsA" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/AuthoritiesStepsB" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterFamilyName" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterFirstName" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/OtherNameMsg2" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterOtherName" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterStatus" required="false()" type="select1"/>
      <bind nodeset="/un_special_rapporteur/SpecifyOtherStatus" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterAddress" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterPhone" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterFax" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/SubmitterEmail" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/ConfidentialNote" readonly="true()" type="string"/>
      <bind nodeset="/un_special_rapporteur/IDConfidential" required="false()" type="string"/>
      <bind nodeset="/un_special_rapporteur/QuestionnaireDate" required="false()" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind nodeset="/un_special_rapporteur/SignatureReceived" required="false()" type="string"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/un_special_rapporteur/today" type="date" constraint=". >= date('1900-01-01')" jr:constraintMsg="The date you entered is not within the accepted date range."/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/un_special_rapporteur/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
