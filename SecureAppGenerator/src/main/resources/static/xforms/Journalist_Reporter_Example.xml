			<bind nodeset="/data/DraftsSection/EditorsComments" type="xsd:string" />
			<bind nodeset="/data/DraftsSection/Final_Draft" type="xsd:string" />
			<bind nodeset="/data/AttachmentsSection" />
			<bind nodeset="/data/AttachmentsSection/Attachments__background" />
			<bind nodeset="/data/AttachmentsSection/Attachments_published" />
			<bind nodeset="/data/AttachmentsSection/AttachmentsNotes" type="xsd:string" />
		</model>
	</h:head>
