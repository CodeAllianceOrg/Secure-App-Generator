<?xml version="1.0" encoding="UTF-8" ?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Untitled Form</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/07E95BA1-0F74-4A32-A099-C1CD8F4045A3" uiVersion="1" version="1" name="Untitled Form">
					<General_Information>
						<Language />
						<Assignment />
						<Source_Title />
						<Source_CompanyOrganizationAffiliation />
						<Source_PR />
						<Source_Contact_Information />
						<Source_Social_Media_Coordinates />
						<Date_of_Interview />
						<Location_of_Interview />
						<Does_interviewee_wish_to_remain_anonymous />
					</General_Information>
					<Story_Information>
						<Story_Title />
						<Story_Date />
						<Story_Keywords />
						<Media_Outlet />
						<Contact_URL />
						<EditorName />
						<EditorTitle />
						<EditorContactInfo />
					</Story_Information>
					<Notes>
						<InterviewNotes />
					</Notes>
					<DraftsSection>
						<Drafts />
						<EditorsComments />
						<Final_Draft />
					</DraftsSection>
					<AttachmentsSection>
						<Attachments__background />
						<Attachments_published />
						<AttachmentsNotes />
					</AttachmentsSection>
				</data>
			</instance>
			<bind nodeset="/data/General_Information" />
			<bind nodeset="/data/General_Information/Language" />
			<bind nodeset="/data/General_Information/Assignment" type="xsd:string" />
			<bind nodeset="/data/General_Information/Source_Title" type="xsd:string" />
