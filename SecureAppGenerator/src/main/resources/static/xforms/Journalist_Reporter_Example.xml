			<bind nodeset="/data/General_Information/Date_of_Interview" type="xsd:date" />
			<bind nodeset="/data/General_Information/Location_of_Interview" type="xsd:string" />
			<bind nodeset="/data/General_Information/Does_interviewee_wish_to_remain_anonymous" />
			<bind nodeset="/data/Story_Information" />
			<bind nodeset="/data/Story_Information/Story_Title" type="xsd:string" />
			<bind nodeset="/data/Story_Information/Story_Date" type="xsd:date" />
			<bind nodeset="/data/Story_Information/Story_Keywords" type="xsd:string" />
			<bind nodeset="/data/Story_Information/Media_Outlet" type="xsd:string" />
			<bind nodeset="/data/Story_Information/Contact_URL" type="xsd:string" />
			<bind nodeset="/data/Story_Information/EditorName" type="xsd:string" />
